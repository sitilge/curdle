  "keywords": ["php", "http", "rest", "database"],
  "license": "MIT",
  "homepage": "http://curdle.sitilge.id.lv",
  "support": {
    "issues": "https://github.com/sitilge/curdle/issues",
    "source": "https://github.com/sitilge/curdle"
