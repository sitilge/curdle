    "propel/propel": "~2.0@dev",
    "filp/whoops": "^2.1"
  },
  "require-dev" : {
    "phpunit/phpunit": "5.1.*",
    "fzaninotto/faker": "~1.4",
    "friendsofphp/php-cs-fixer": "^1.11",
    "phploc/phploc": "*",
    "phpmd/phpmd" : "@stable"
  },
  "autoload": {
    "psr-4": {
      "Propeller\\": "src/"
